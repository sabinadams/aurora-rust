generator js {
    provider        = "prisma-client-js"
    previewFeatures = ["extendedIndexes"]
}

generator js2 {
    provider        = "prisma-client-js"
    previewFeatures = ["extendedIndexes"]
}

enum Test {
    User
    Admin
    Guest
}

datasource db {
    provider = "mongodb"
    url      = "mongodb://"
}

model Test2 {
    id      String @id @map("_id")
    content Json   @default("{}") @db.Json
}
